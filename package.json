{
  "name": "scout-game",
  "version": "0.0.0",
  "type": "module",
  "browserslist": [
    "defaults"
  ],
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "prisma:link": "rm -rf ./node_modules/.prisma && npm link @charmverse/core && npm run prisma:generate",
    "prisma:migrate": "cd node_modules/@charmverse/core && npm run prisma:migrate",
    "prisma:studio": "cd node_modules/@charmverse/core && npm run prisma:studio",
    "prisma:generate": "cd node_modules/@charmverse/core && npm run prisma:generate",
    "prisma:reset": "cd node_modules/@charmverse/core && npm run prisma:reset",
    "prisma:dev": "cd node_modules/@charmverse/core && npm run prisma:migrate",
    "prisma:deploy": "cd node_modules/@charmverse/core && npm run prisma:deploy",
    "postinstall": "cd node_modules/@charmverse/core && npm run postinstall",
    "prepare": "husky install",
    "scoutgame": "npx react-env --path .env.local --dest ./apps/scoutgame/public -- npm run start --workspace=apps/scoutgame",
    "scoutgame:remote": "npx react-env --path .env.production --dest ./apps/scoutgame/public -- npm run start --workspace=apps/scoutgame",
    "scoutgame:staging": "npx react-env --path .env.staging.local --dest ./apps/scoutgame/public -- npm run start --workspace=apps/scoutgame",
    "scoutgame:build": "npm run build --w=apps/scoutgame",
    "scoutgame:start:prod": "npx react-env --path .env.local --dest ./apps/scoutgame/public -- npm run start:prod --workspace=apps/scoutgame",
    "scoutgame:test": "dotenv -e .env.test.local -- npm test -w apps/scoutgame",
    "scoutgame:e2e": "npx dotenv -e .env.test.local -- npm run test:e2e -w=apps/scoutgame -- --headed",
    "scoutgame:e2e:ci": "npx dotenv -e .env.test.local -- npm run test:e2e -w=apps/scoutgame",
    "scoutgame:start:test": "npm run start:test -w=apps/scoutgame",
    "scoutgame:start:test:ci": "npm run start:test:ci -w=apps/scoutgame",
    "onchain-scoutgame:build": "npm run build --w=apps/onchain-scoutgame",
    "onchain-scoutgame:start:prod": "npx react-env --path .env.local --dest ./apps/onchain-scoutgame/public -- npm run start:prod --workspace=apps/onchain-scoutgame",
    "onchain-scoutgame:test": "dotenv -e .env.test.local -- npm test -w apps/onchain-scoutgame",
    "onchain-scoutgame:e2e": "npx dotenv -e .env.test.local -- npm run test:e2e -w=apps/onchain-scoutgame -- --headed",
    "onchain-scoutgame:e2e:ci": "npx dotenv -e .env.test.local -- npm run test:e2e -w=apps/onchain-scoutgame",
    "onchain-scoutgame:start:test": "npm run start:test -w=apps/onchain-scoutgame",
    "onchain-scoutgame:start:test:ci": "npm run start:test:ci -w=apps/onchain-scoutgame",
    "onchain-scoutgame": "npx react-env --path .env.local --dest ./apps/onchain-scoutgame/public -- npm run start --workspace=apps/onchain-scoutgame",
    "cron:test": "dotenv -e .env.test.local -- npm test -w apps/cron",
    "cron:staging": "npm run start:prod:node18 -w apps/cron",
    "cron:prod": "npm run start:prod:node18 -w apps/cron",
    "onchain-cron:test": "dotenv -e .env.test.local -- npm test -w apps/onchain-cron",
    "onchain-cron:staging": "npm run start:prod:node18 -w apps/onchain-cron",
    "onchain-cron:prod": "npm run start:prod:node18 -w apps/onchain-cron",
    "admin": "npx react-env --path .env.local --dest ./apps/admin/public -- npm start -w=apps/admin",
    "admin:remote": "npx react-env --path .env.production --dest ./apps/admin/public -- npm start -w=apps/admin",
    "admin:test": "dotenv -e .env.test.local -- npm test -w apps/admin",
    "draft": "npx react-env --path .env.local --dest ./apps/draft/public -- npm start -w=apps/draft",
    "draft:remote": "npx react-env --path .env.production --dest ./apps/draft/public -- npm start -w=apps/draft",
    "draft:test": "dotenv -e .env.test.local -- npm test -w apps/draft",
    "generate:package": "chmod +x ./scripts/generatePackage.sh && ./scripts/generatePackage.sh",
    "agents": "dotenv -e .env.local -- npm run start -w=apps/agents",
    "agents:staging": "npm run start:prod:node18 -w apps/agents",
    "agents:prod": "npm run start:prod:node18 -w apps/agents",
    "agents:test": "dotenv -e .env.test.local -- npm test -w apps/agents",
    "seed:staging": "dotenv -e .env.staging -- tsx apps/cron/src/scripts/seeder/seedWithRealCharmverseGithubData.ts"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.12.11",
    "@beam-australia/react-env": "^3.1.1",
    "@charmverse/core": "^0.121.0",
    "@datadog/browser-rum": "^5.33.0",
    "@emotion/styled": "^11.13.5",
    "@mui/icons-material": "^6.1.4",
    "@mui/lab": "^6.0.0-beta.11",
    "@mui/material": "^6.1.4",
    "@mui/material-nextjs": "^6.1.4",
    "@mui/system": "^6.1.4",
    "@mui/x-date-pickers": "^7.20.0",
    "@mui/x-tree-view": "^7.20.0",
    "@svgr/webpack": "^8.1.0",
    "@tanstack/react-query": "^5.61.3",
    "@vercel/og": "^0.6.3",
    "aws-cdk-lib": "^2.187.0",
    "dd-trace": "^5.33.0",
    "ethers": "^6.13.5",
    "iron-session": "^8.0.4",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "material-ui-popup-state": "^5.3.1",
    "next": "^14.2.26",
    "next-view-transitions": "^0.3.4",
    "pino-pretty": "^13.0.0",
    "react": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "react-icons": "^5.4.0",
    "sass": "^1.81.0",
    "sharp": "^0.33.5",
    "swr": "^2.2.5",
    "uuid": "^11.0.3",
    "viem": "^2.22.17",
    "wagmi": "^2.14.11",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@next/eslint-plugin-next": "^14.2.15",
    "@playwright/test": "^1.47.1",
    "@stylistic/eslint-plugin-ts": "^2.9.0",
    "@swc/core": "^1.9.3",
    "@swc/jest": "^0.2.37",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.5",
    "@types/koa__cors": "^5.0.0",
    "@types/koa__router": "^8.0.11",
    "@types/koa-router": "^7.4.8",
    "@types/lodash": "^4.17.13",
    "@types/luxon": "2.4",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/parser": "^8.9.0",
    "cd": "^0.3.3",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "encoding": "^0.1.13",
    "esbuild": "^0.25.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^14.2.15",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.9.0",
    "husky": "^7.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^12.3.2",
    "node-mocks-http": "^1.14.1",
    "playwright": "^1.47.1",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "typescript": "^5.4.2"
  }
}
